# Sensitive credentials for Proxmox Kubernetes cluster
# Copy this file to credentials.tfvars and replace with your actual values
# NEVER commit this file to version control with real credentials

proxmox_credential = {
  api_token_id     = "terraform@pve!terraform"
  api_token_secret = "your-proxmox-api-token-secret-here"
}

vm_credential = {
  username = "ubuntu"
  password = "your-secure-vm-password"
  ssh_keys = "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAABAQC... your-ssh-public-key-here"
  ssh_private_key_path = "/home/root/.ssh/rsa"
}
